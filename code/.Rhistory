norm2 <- function(matr){
matr <- matr[which(rowSums(matr) > 0),]
matr <- DGEList(matr)
matr <- calcNormFactors(matr, method = "TMM")
tmp <- voom(matr)$E
t(apply(tmp, 1, function(x) (x - mean(x))/sd(x)))
}
